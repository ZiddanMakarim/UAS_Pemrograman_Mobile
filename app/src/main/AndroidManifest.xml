<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZiddanDatabaseAPIProject"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ZiddanDatabaseAPIProject.NoActionBar"/>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </application>
        <activity
            android:name="com.smartneasy.hargahponline.FormHandphone"
            android:label="@string/title_activity_add_new"
            tools:ignore="Instantiatable,WrongManifestParent">
        </activity>
        <activity
            android:name="com.smartneasy.hargahponline.DetailHandphone"
            android:label="@string/title_activity_detail_handphone"
            tools:ignore="WrongManifestParent">
        </activity>
    </manifest>
